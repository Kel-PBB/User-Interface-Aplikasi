<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/birtud">


    <LinearLayout
        android:layout_width="158dp"
        android:layout_height="3dp"
        android:background="@color/white"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="537dp"
        tools:ignore="MissingConstraints"></LinearLayout>

    <ImageView
        android:layout_width="250dp"
        android:layout_height="191dp"
        android:background="@drawable/logo"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="59dp"
        tools:ignore="ContentDescription,MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="170dp"
        android:layout_height="43dp"
        android:text="Cloud Rent"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="italic"
        tools:ignore="HardcodedText,MissingConstraints,SpUsage,TextSizeCheck,VisualLintOverlap"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="207dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="170dp"
        android:layout_height="43dp"
        android:text="Cloud Rent"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="italic"
        tools:ignore="DuplicateIds,HardcodedText,MissingConstraints,SpUsage,TextSizeCheck"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="207dp" />

    <android.support.design.widget.TextInputLayout
        android:id="@+id/textInputLayout"
        android:layout_width="338dp"
        android:layout_height="53dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="291dp">

        <android.support.design.widget.TextInputEditText
            android:id="@+id/email"
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:background="@color/white"
            android:hint="   Email or Username"
            android:textColor="@color/black"
            android:textSize="15dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,TouchTargetSizeCheck" />
    </android.support.design.widget.TextInputLayout>

    <Button
        android:id="@+id/button2"
        android:layout_width="113dp"
        android:layout_height="51dp"
        android:text="Login"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:background="@color/birmud"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="450dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <Button
        android:id="@+id/button3"
        android:layout_width="248dp"
        android:layout_height="50dp"
        android:background="@color/birmud"
        android:text="Google"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="564dp"
        tools:ignore="HardcodedText,MissingConstraints" />



    <Button
        android:id="@+id/button4"
        android:layout_width="248dp"
        android:layout_height="52dp"
        android:background="@color/birmud"
        android:text="Facebook"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="626dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/Password"
        android:layout_width="335dp"
        android:layout_height="40dp"
        android:background="@color/white"
        android:ems="10"
        android:hint="    Password"
        android:textSize="15dp"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="369dp"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:layout_width="158dp"
        android:layout_height="3dp"
        android:background="@color/white"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="-38dp"
        tools:layout_editor_absoluteY="537dp"
        tools:ignore="MissingConstraints"></LinearLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tidak Punya Akun?"
        android:textColor="@color/white"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="529dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="56dp"
        android:layout_height="14dp"
        android:text="REGISTRASI"
        android:textColor="@color/white"
        android:textSize="9dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="219dp"
        tools:layout_editor_absoluteY="529dp"
        tools:ignore="MissingConstraints" />


</android.support.constraint.ConstraintLayout>