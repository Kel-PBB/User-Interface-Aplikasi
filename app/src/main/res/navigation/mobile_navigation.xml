<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.cloudrent.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/navigation_loc"
        android:name="com.example.cloudrent.LocationFragment"
        android:label="@string/title_locations"
        tools:layout="@layout/fragment_location" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.cloudrent.NotificcationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notificcations"/>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.cloudrent.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

</navigation>